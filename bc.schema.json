{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "message": {
      "type": "string"
    },
    "type": {
      "type": "integer",
      "minimum": 0,
      "maximum": 2
    },
    "highPriority": {
      "type": "boolean"
    },
    "actionLabel": {
      "type": "string"
    },
    "actionCommand": {
      "type": "string"
    }
  },
  "required": [
    "title",
    "message"
  ]
}